{
    // Colors
    "workbench.colorCustomizations": {
        
    },
    "editor.tokenColorCustomizations": {
        "keywords": {
            "fontStyle": "italic"
        }
    },
   
    // Settings
    "editor.matchBrackets": "never",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.fontWeight": "normal",
    "terminal.integrated.fontWeight": "normal",
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "none",
    "editor.renderIndentGuides": false,
    "editor.renderLineHighlight": "none",
    "editor.overviewRulerBorder": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.folding": false,
    "editor.occurrencesHighlight": false,
    "explorer.openEditors.visible": 0,
    "workbench.activityBar.visible": false,
    "workbench.editor.showIcons": false,
    "workbench.editor.tabCloseButton": "off",
    "indenticator.color.dark": "rgba(255,255,255,.1)",
    "window.menuBarVisibility": "toggle",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "explorer.confirmDelete": false,
    "files.autoSave": "afterDelay",
    "explorer.confirmDragAndDrop": false,
    "git.enabled": false,
    "problems.decorations.enabled": false,
    "outline.problems.colors": false,
    "outline.problems.badges": false,
    "outline.problems.enabled": false,
    "editor.lineHeight": 20,
    "editor.fontSize": 13,
    "workbench.editor.showTabs": false,
    "workbench.editor.pinnedTabSizing": "shrink",
    "C_Cpp.default.includePath": [
        "include"
    ],
    "[luxury]": {
    
    },
    "workbench.iconTheme": null,
    "workbench.colorTheme": "Solarized Monochrome Dark",
    "workbench.statusBar.visible": false,
    "editor.fontFamily": "Roboto Mono, Source Code Pro, Cartograph CF, Consolas, 'Courier New', monospace",
    "terminal.integrated.fontFamily": "Roboto Mono, Source Code Pro, Cartograph CF, Consolas, 'Courier New', monospace",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.fontWeightBold": "normal",
    "autoHide.autoHideSideBar": false,
    "autoHide.panelDelay": 0
}
